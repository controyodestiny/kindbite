# KindBite Frontend Environment Variables
# Copy this file to .env.local and update values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8002/api/v1
REACT_APP_API_TIMEOUT=10000

# Authentication
REACT_APP_TOKEN_STORAGE_KEY=kindbite_tokens
REACT_APP_USER_STORAGE_KEY=kindbite_user

# Development
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# Optional: Analytics & Monitoring
# REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id
# REACT_APP_SENTRY_DSN=your_sentry_dsn
